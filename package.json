{
  "name": "ember-l10n",
  "version": "4.3.1",
  "description": "A generic localization solution for ember projects using gettext.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint ./addon ./app ./tests/dummy/app",
    "lint:js": "eslint .",
    "run-prettier": "prettier --write \"**/*.{js,scss}\"",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:ember-one": "ember try:one",
    "test:node": "node node-tests/index.js"
  },
  "repository": "https://github.com/Cropster/ember-l10n",
  "engines": {
    "node": "10.* || >= 12"
  },
  "author": "Cropster",
  "license": "MIT",
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.2.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.20.0",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.20.2",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.9.1",
    "ember-truth-helpers": "^2.1.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-ember-suave": "^1.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "glob": "^7.1.3",
    "loader.js": "^4.7.0",
    "mocha": "^8.1.1",
    "npm-run-all": "^4.1.5",
    "pretender": "^3.4.3",
    "prettier": "^2.0.5",
    "qunit-dom": "^1.2.0",
    "rimraf": "^3.0.2"
  },
  "keywords": [
    "ember-addon",
    "localization",
    "translations",
    "gettext",
    "i18n",
    "l10n",
    "json",
    "po"
  ],
  "dependencies": {
    "@babel/parser": "^7.5.5",
    "@glimmer/syntax": "^0.56.0",
    "ast-types": "^0.13.2",
    "broccoli-plugin": "^4.0.3",
    "chalk": "^4.1.0",
    "ember-cli-babel": "^7.21.0",
    "ember-cli-htmlbars": "^5.2.0",
    "ember-get-config": "^0.2.4",
    "gettext-parser": "^4.0.2",
    "inquirer": "^7.0.4",
    "shelljs": "^0.8.2"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-uglify"
    ]
  }
}
