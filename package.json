{
  "name": "ember-l10n",
  "version": "3.2.1",
  "description": "A generic localization solution for ember projects using gettext.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint ./addon ./app ./tests/dummy/app",
    "lint:js": "eslint .",
    "run-prettier": "prettier --write \"**/*.{js,scss}\"",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "node-tests": "node node-tests/index.js"
  },
  "repository": "https://github.com/Cropster/ember-l10n",
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "author": "Cropster",
  "license": "MIT",
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.2.0",
    "ember-cli": "~3.11.0",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-ifa": "^0.7.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-pretender": "^3.0.0",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.11.1",
    "ember-source-channel-url": "^1.1.0",
    "ember-template-lint": "~1.5.0",
    "ember-truth-helpers": "^2.1.0",
    "ember-try": "^1.0.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-ember-suave": "^1.0.0",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "glob": "^7.1.3",
    "husky": "^1.3.1",
    "loader.js": "^4.7.0",
    "mocha": "^5.2.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.16.1",
    "qunit-dom": "^0.8.4",
    "rimraf": "^2.6.2"
  },
  "keywords": [
    "ember-addon",
    "localization",
    "translations",
    "gettext",
    "i18n",
    "l10n",
    "json",
    "po"
  ],
  "dependencies": {
    "@babel/parser": "^7.5.5",
    "@glimmer/syntax": "^0.38.0",
    "ast-types": "^0.13.2",
    "chalk": "^2.4.1",
    "ember-cli-babel": "^7.7.3",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-get-config": "^0.2.4",
    "gettext-parser": "^3.0.0",
    "inquirer": "^6.2.1",
    "shelljs": "^0.8.2"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cli-ifa"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  }
}
