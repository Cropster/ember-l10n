{
  "name": "ember-l10n",
  "version": "5.1.0",
  "description": "A generic localization solution for ember projects using gettext.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint ./addon ./app ./tests/dummy/app",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "run-prettier": "prettier --write \"**/*.{js,scss}\"",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:ember-one": "ember try:one",
    "test:node": "node node-tests/index.js"
  },
  "repository": "https://github.com/Cropster/ember-l10n",
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "author": "Cropster",
  "license": "MIT",
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.2",
    "@embroider/test-setup": "^0.43.5",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.2.0",
    "ember-auto-import": "^1.11.3",
    "ember-cli": "~3.28.3",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.6.0",
    "ember-truth-helpers": "^3.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-ember-suave": "^1.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "glob": "^7.1.3",
    "loader.js": "^4.7.0",
    "mocha": "^8.1.1",
    "npm-run-all": "^4.1.5",
    "pretender": "^3.4.3",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "qunit-dom": "^1.6.0",
    "rimraf": "^3.0.2"
  },
  "keywords": [
    "ember-addon",
    "localization",
    "translations",
    "gettext",
    "i18n",
    "l10n",
    "json",
    "po"
  ],
  "dependencies": {
    "@babel/parser": "^7.12.3",
    "@glimmer/component": "^1.0.4",
    "@glimmer/syntax": "^0.62.3",
    "@glimmer/tracking": "^1.0.4",
    "ast-types": "^0.14.2",
    "broccoli-plugin": "^4.0.3",
    "chalk": "^4.1.0",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "gettext-parser": "^4.0.2",
    "inquirer": "^7.0.4",
    "shelljs": "^0.8.4"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-uglify",
      "ember-cli-terser"
    ]
  }
}